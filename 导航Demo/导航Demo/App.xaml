<prism:PrismApplication
    x:Class="导航Demo.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:converter="clr-namespace:导航Demo.Converters"
    xmlns:local="clr-namespace:导航Demo"
    xmlns:prism="http://prismlibrary.com/">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Styles/LightTheme.xaml" />
                <ResourceDictionary Source="./Styles/ButtonStyles.xaml" />

                <ResourceDictionary Source="./Assets/Icons/Icons.xaml" />

            </ResourceDictionary.MergedDictionaries>


            <!--  注册转换器  -->
            <converter:GeometryResourceConverter x:Key="GeometryResourceConverter" />
        </ResourceDictionary>

    </Application.Resources>
</prism:PrismApplication>
