<prism:PrismApplication x:Class="OveralllAuth__V1._0__WPF.App"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:local="clr-namespace:OveralllAuth__V1._0__WPF"
                        xmlns:converter="clr-namespace:Navigation.Converters"
                        xmlns:prism="http://prismlibrary.com/" xmlns:converter1="clr-namespace:Navigation.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="./Styles/LightTheme.xaml"/>
                <ResourceDictionary Source="./Styles/ButtonStyles.xaml"/>

                <ResourceDictionary Source="./Assets/Icons/Icons.xaml"/>

            </ResourceDictionary.MergedDictionaries>


            <!-- 注册转换器 -->
            <converter:GeometryResourceConverter x:Key="GeometryResourceConverter" />
        </ResourceDictionary>

    </Application.Resources>
</prism:PrismApplication>
